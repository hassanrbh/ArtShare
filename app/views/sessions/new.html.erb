<h2>Sign-in</h2>
<form action="<%= session_path %>" method="POST">
    <%= auth_token %>
    <br />
    <label for="email">Email</label>
    <input type="email" id="email" name="users[email]" />
    <br />
    <label for="password">Password</label>
    <input type="password" id="password" name="users[password]" />
    <br />

    <input type="submit" value="Envoye"/>
</form>